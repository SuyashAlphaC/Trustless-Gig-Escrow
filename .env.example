# ===========================================
# MNEE Gig Escrow - Environment Configuration
# ===========================================

# Network RPC URLs
SEPOLIA_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/YOUR_API_KEY
MAINNET_RPC_URL=https://eth-mainnet.g.alchemy.com/v2/YOUR_API_KEY
POLYGON_RPC_URL=https://polygon-mainnet.g.alchemy.com/v2/YOUR_API_KEY

# Deployer Private Key (without 0x prefix)
# WARNING: Never commit your actual private key!
PRIVATE_KEY=your_private_key_here

# Chainlink Functions Configuration
# Get your subscription ID from https://functions.chain.link/
CHAINLINK_SUBSCRIPTION_ID=0

# MNEE Token Address (per network)
# Replace with actual MNEE token addresses
MNEE_TOKEN_ADDRESS=0x0000000000000000000000000000000000000000

# Etherscan API Key (for contract verification)
ETHERSCAN_API_KEY=your_etherscan_api_key

# Polygonscan API Key (for Polygon verification)
POLYGONSCAN_API_KEY=your_polygonscan_api_key

# Arbiscan API Key (for Arbitrum verification)
ARBISCAN_API_KEY=your_arbiscan_api_key

# Basescan API Key (for Base verification)
BASESCAN_API_KEY=your_basescan_api_key
